# vim: filetype=sh
alias cac="cd ${XDG_CACHE_HOME:-$HOME/.cache} && ls" \
cf="cd ${XDG_CONFIG_HOME:-$HOME/.config} && ls" \
D="cd ${XDG_DOWNLOAD_DIR:-$HOME/Downloads} && ls" \
