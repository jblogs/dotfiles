S="cd ${XDG_DATA_HOME:-$HOME/.local/share} && ls" \
h="cd $HOME && ls" \
m="cd ${XDG_MUSIC_DIR:-$HOME/Music} && ls" \
mn="cd /mnt && ls" \
pp="cd ${XDG_PICTURES_DIR:-$HOME/Pictures} && ls" \
