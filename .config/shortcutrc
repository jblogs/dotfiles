src="cd $HOME/.local/src && ls" \
t="cd $HOME/temp && ls" \
pw="cd ${XDG_DATA_HOME:-$HOME/.local/share}/password-store && ls" \
vv="cd ${XDG_VIDEOS_DIR:-$HOME/Videos} && ls" \
